{
  "name": "selena",
  "version": "0.2.0",
  "description": "A textual language that compiles to UML sequence diagrams.",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "tsc --noEmit && eslint --ignore-path .gitignore .",
    "lint-fix": "tsc --noEmit && eslint --fix --ignore-path .gitignore .",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.test.*\"",
    "coverage": "c8 --reporter=lcov --all --include \"src/**/*.ts\" npm test",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/selena.git"
  },
  "keywords": [
    "compiler",
    "uml",
    "sequence",
    "diagram"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/selena/issues"
  },
  "homepage": "https://github.com/meyfa/selena#readme",
  "devDependencies": {
    "@types/chai": "4.2.22",
    "@types/mocha": "9.0.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "c8": "7.11.3",
    "chai": "4.3.4",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "37.0.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "mocha": "9.1.3",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  }
}
