{
  "name": "selena",
  "version": "0.0.0",
  "description": "A textual language that compiles to UML sequence diagrams.",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf \"dist/*\"",
    "build": "npm run clean && tsc",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ignore-path .gitignore .",
    "test": "npm run clean && ts-mocha --recursive \"test/**/*.test.*\"",
    "coverage": "nyc --reporter=lcov npm test",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/selena.git"
  },
  "keywords": [
    "compiler",
    "uml",
    "sequence",
    "diagram"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/selena/issues"
  },
  "homepage": "https://github.com/meyfa/selena#readme",
  "devDependencies": {
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "chai": "^4.3.4",
    "eslint": "^7.28.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.3.3"
  }
}
